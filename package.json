{
  "name": "ddd-react",
  "version": "1.6.4",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "dev": "npm run type-check && rollup -c -w --serve",
    "build": "tsc && vite build && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "npx eslint ."
  },
  "keywords": [
    "react",
    "ddd",
    "library",
    "frontend",
    "typescript"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.mjs",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-dev-runtime.mjs",
      "types": "./dist/jsx-dev-runtime.d.ts"
    }
  },
  "author": "DDD Team",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "license": "ISC",
  "description": "The minimalistic React-like library made by DDD team v2",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@types/node": "^24.8.1",
    "rollup": "^4.52.4",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-filesize": "^10.0.0",
    "semantic-release": "^24.2.9",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.3.0",
    "eslint": "^9.38.0"
  },
  "dependencies": {
    "@guanghechen/fast-deep-equal": "^2.3.7"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
